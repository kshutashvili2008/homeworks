from turtle import *

shape("turtle")
speed(10)
color("dark blue")
begin_fill()

forward(100)
left(90)

forward(100)
left(90)

forward(100)
left(90)


forward(100)
end_fill()
left(90)

forward(35)
color("red")
begin_fill()
left(90)

forward(40)
right(90)

forward(25)
right(90)

forward(40)
end_fill()

color("dark blue")

left(90)
forward(40)

left(90)
forward(100)
color("purple")
begin_fill()

left(30)
forward(100)

left(120)
forward(100)

end_fill()

left(30)
color("dark blue")
forward(100)

left(90)
forward(35)
color("red")
left(90)
forward(40)
color("dark blue")

forward(25)
color("light blue")
begin_fill()

forward(20)
left(90)

forward(20)
left(90)

forward(20)
left(90)

forward(20)
left(90)

end_fill()

right(90)
color("dark blue")
forward(25)
color("light blue")
begin_fill()
forward(20)
left(90)

forward(20)
left(90)

forward(20)
left(90)

forward(20)
left(90)
end_fill()
exitonclick()